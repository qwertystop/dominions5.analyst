<table>
  <tr>
    <th>Offset</th>
    <th>Name</th>
    <th>Type</th>
    <th>Value</th>
  </tr>
  <%= for {offset, name, type, value} <- contents do %>
    <tr>
      <td><%= offset %></td>
      <td><%= name %></td>
      <td><%= type %></td>
      <td><%= value %></td>
    </tr>
  <%end%>
</table>
